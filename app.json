{
  "expo": {
    "name": "Urbanify",
    "slug": "Urbanify",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/IconProvisorio.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/LogoProvisoria.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/IconProvisorio.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.diegoleandro477.Urbanify"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-secure-store",
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "ff1a416c-097d-421f-af95-b6da449f74dd"
      }
    }
  }
}
